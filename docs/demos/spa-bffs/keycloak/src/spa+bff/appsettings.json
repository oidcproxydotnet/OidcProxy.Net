{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "OidcProxy": {
    "Auth0": {
      "ClientId": "{yourClientId}",
      "ClientSecret": "{yourClientSecret}",
      "Domain": "{yourDomain}",
      "Audience": "{yourAudience}",
      "FederatedLogout": false,
      "Scopes": [
        "openid",
        "profile",
        "offline_access"
      ]
    },
    "ReverseProxy": {
      "Routes": {
        "api": {
          "ClusterId": "api",
          "Match": {
            "Path": "/api/{*any}"
          }
        }
      },
      "Clusters": {
        "api": {
          "Destinations": {
            "api/node1": {
              "Address": "http://localhost:8080/"
            }
          }
        }
      }
    }
  }
}
